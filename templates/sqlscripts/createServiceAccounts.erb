IF NOT EXISTS(select * from sys.server_principals r where name = '<%= @dbuserprefix %>Aims')
BEGIN
  print 'Creating account <%= @dbuserprefix %>Aims'
  CREATE LOGIN [<%= @dbuserprefix %>Aims] WITH PASSWORD=N'<%= @srvlaimspass %>', DEFAULT_DATABASE=[master], CHECK_EXPIRATION=OFF, CHECK_POLICY=OFF
  EXEC master..sp_addsrvrolemember @loginame = N'<%= @dbuserprefix %>Aims', @rolename = N'sysadmin'
END

